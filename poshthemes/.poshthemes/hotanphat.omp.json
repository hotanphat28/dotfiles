{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue62a"
          },
          "leading_diamond": "\ue0c7\u2593",
          "template": " {{ .Icon }} ",
          "foreground": "#101010",
          "background": "#FFC90E",
          "type": "os",
          "style": "diamond"
        },
        {
          "type": "shell",
          "style": "diamond",
          "foreground": "#101010",
          "background": "#FFC90E",
          "leading_diamond": "\uE0B0<#101010>\ue0b1</>",
          "trailing_diamond": "\uE0B0",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "\uebc7 pwsh",
              "bash": "\uea85 bash"
            }
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "template": "<#101010>\ue0b1</> \uE235 {{ .Venv }} ({{ .Full }}) ",
          "foreground": "#101010",
          "background": "#FFC90E",
          "trailing_diamond": "\uE0B0",
          "properties": {
            "display_mode": "environment",
            "fetch_virtual_env": true
          }
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "<#FFC90E> \uee30 {{ .UserName }}</><#FFC90E> \ue0b1</>",
          "foreground": "#FFC90E",
          "background": "#101010",
          "type": "session",
          "style": "diamond",
          "leading_diamond": "\ue0c7\u2593"
        },
        {
          "template": " \uf0e7<#FFC90E> \ue0b1</>",
          "foreground": "#FFC90E",
          "background": "#101010",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "folder_icon": "<#FFC90E>\ue5fe</>",
            "folder_separator_icon": "<#FFC90E> \ue0b1 </>",
            "home_icon": "<#FFC90E>\uf015</> ",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#f4f4f4",
          "background": "#101010",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "<#FFC90E>\ue0b1 </>{{ .UpstreamIcon }} {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }}<#ED1C24> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#22B14C> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#FFC90E",
          "background": "#101010",
          "type": "git",
          "style": "diamond",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFC90E{{ end }}",
            "{{ if gt .Ahead 0 }}#0EFFC9{{ end }}",
            "{{ if gt .Behind 0 }}#C90EFF{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "\uE0B0",
          "foreground": "#101010"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
